  script.
    document.querySelectorAll('.card').forEach(card => {
      const img = card.querySelector('img');
      const images = card.dataset.images?.split(',');
      let index = 0;

      if (!images) return;

      card.addEventListener('click', () => {
        index = (index + 1) % images.length;
        img.classList.remove('image-change');
        void img.offsetWidth;
        img.src = images[index];
        img.classList.add('image-change');
      });
    });


  script.
    document.querySelectorAll('.card').forEach(card => {
      const images = Array.from(card.querySelectorAll('img'));
      let current = 0;
      let interval;

      // Set initial transform
      images.forEach((img, i) => {
        img.style.transform = i === 0 ? 'translateY(0)' : 'translateY(100%)';
      });

      // Function to blur other cards
      const blurOthers = () => {
        document.querySelectorAll('.card').forEach(c => {
          if (c !== card) c.classList.add('blur');
          else c.classList.remove('blur');
        });
      };

      // Function to remove blur from all cards
      const resetBlur = () => {
        document.querySelectorAll('.card').forEach(c => c.classList.remove('blur'));
      };

      // Hover: start slider + blur others
      card.addEventListener('mouseenter', () => {
        blurOthers();
        interval = setInterval(() => {
          current = (current + 1) % images.length;
          images.forEach((img, i) => {
            img.style.transform = i === current ? 'translateY(0)' : 'translateY(100%)';
          });
        }, 500);
      });

      // Hover leave: stop slider + remove blur
      card.addEventListener('mouseleave', () => {
        clearInterval(interval);
        resetBlur();
      });

      // Click: advance image + blur others
      card.addEventListener('click', () => {
        blurOthers();
        current = (current + 1) % images.length;
        images.forEach((img, i) => {
          img.style.transform = i === current ? 'translateY(0)' : 'translateY(100%)';
        });
      });
    });

  script.
      const textElements = document.querySelectorAll(".animate-text-unique");

      textElements.forEach(textElement => {
        // Split text into words
        const words = textElement.textContent.split(" ");

        // Wrap each word in a span and add a trailing space
        const wrappedWords = words.map(word => `<span>${word}&nbsp;</span>`).join("");

        // Put the words back
        textElement.innerHTML = wrappedWords;

        // Wrap words per "row"
        function wrapLines(el) {
          const words = Array.from(el.querySelectorAll("span"));
          let lines = [];
          let currentLineTop = words[0].offsetTop;
          let line = [];

          words.forEach(word => {
            if (word.offsetTop !== currentLineTop) {
              lines.push(line);
              line = [];
              currentLineTop = word.offsetTop;
            }
            line.push(word);
          });

          lines.push(line);

          // Wrap each line in a div
          lines.forEach(lineWords => {
            const lineWrapper = document.createElement("div");
            lineWrapper.classList.add("line");
            lineWords[0].parentNode.insertBefore(lineWrapper, lineWords[0]);
            lineWords.forEach(w => lineWrapper.appendChild(w));
          });
        }

        wrapLines(textElement);
      });

      // Animate all lines with GSAP
      gsap.to(".line", {
        duration: 0.8,
        y: "0%",
        opacity: 1,
        stagger: 0.4,
        ease: "power3.out",
        overwrite: "auto",
        onStart: function() {
          this.targets().forEach(line => {
            gsap.to(line.querySelectorAll("span"), {y: "0%", opacity: 1, duration: 0.6, stagger: 0.05});
          });
        }
      });



      //- GSAP Scripts
  script(src="https://cdn.jsdelivr.net/npm/gsap@3.12.2/dist/gsap.min.js")
  script(src="https://cdn.jsdelivr.net/npm/gsap@3.12.2/dist/ScrollTrigger.min.js")
   script.
      gsap.registerPlugin(ScrollTrigger);

      const sections = document.querySelectorAll(".iconic-outer");

      // Stack sections with z-index and set initial position
      sections.forEach((section, i) => {
        gsap.set(section, { zIndex: i + 1, yPercent: i === 0 ? 0 : 100 });
      });

      const tl = gsap.timeline({
        scrollTrigger: {
          trigger: ".iconic-scroll-container",
          start: "top top",
          end: () => `+=${sections.length * 100}%`,
          scrub: 1,
          pin: true
        }
      });

      // Animate all sections
      sections.forEach((section, i) => {
        if (i === 0) return; // skip first section

        const prevSection = sections[i - 1];

        // previous section moves up by 50%, current slides in
        tl.to(prevSection, { yPercent: -50, ease: "none" }, i)
          .to(section, { yPercent: 0, ease: "none" }, i);
      });
