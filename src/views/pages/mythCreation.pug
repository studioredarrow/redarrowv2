  //- SCRIPT: Card Image Slider (click)
  //- GSAP Scripts
  script(src="https://unpkg.com/gsap@3/dist/gsap.min.js")
  script(src="https://unpkg.com/gsap@3/dist/ScrollTrigger.min.js")
  script.
    gsap.registerPlugin(ScrollTrigger);

    gsap.fromTo(
      ".animate-text-unique",
      { y: 40, opacity: 0 },
      {
        y: 0,
        opacity: 1,
        duration: 3.9,
        ease: "power3.out",
        stagger: 0.2,
        scrollTrigger: {
          trigger: ".hero-section-unique",
          start: "top 70%",
          toggleActions: "play none none none"
        }
      }
    );

  script.
    document.querySelectorAll('.card').forEach(card => {
      const img = card.querySelector('img');
      const images = card.dataset.images?.split(',');
      let index = 0;

      if (!images) return;

      card.addEventListener('click', () => {
        index = (index + 1) % images.length;
        img.classList.remove('image-change');
        void img.offsetWidth;
        img.src = images[index];
        img.classList.add('image-change');
      });
    });

  //- SCRIPT: Animate Text
  script.
    document.addEventListener("DOMContentLoaded", () => {
      const elements = document.querySelectorAll(".animate-text-unique");

      elements.forEach((el, elIndex) => {
        const text = el.innerText;
        el.innerHTML = "";

        text.split(" ").forEach((word, index) => {
          const span = document.createElement("span");
          span.className = "word";
          span.style.animationDelay = `${index * 0.08 + elIndex * 0.4}s`;
          span.innerText = word + " ";
          el.appendChild(span);
        });
      });
    }); 

  //- SCRIPT: Card Image Slider (hover)
  script.
    document.querySelectorAll('.card').forEach(card => {
      const images = Array.from(card.querySelectorAll('img'));
      let current = 0;
      let interval;

      images.forEach((img, i) => {
        img.style.transform = i === 0 ? 'translateY(0)' : 'translateY(100%)';
      });

      card.addEventListener('mouseenter', () => {
        interval = setInterval(() => {
          current = (current + 1) % images.length;
          images.forEach((img, i) => {
            img.style.transform = i === current ? 'translateY(0)' : 'translateY(100%)';
          });
        }, 500);
      });

      card.addEventListener('mouseleave', () => {
        clearInterval(interval);
      });

      card.addEventListener('click', () => {
        current = (current + 1) % images.length;
        images.forEach((img, i) => {
          img.style.transform = i === current ? 'translateY(0)' : 'translateY(100%)';
        });
      });
    });

  script(src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.5/gsap.min.js")
  script.
      document.querySelectorAll(".fate-oval").forEach((wrap) => {
        const path = wrap.querySelector(".fate-oval__path");
        const text = wrap.querySelector(".fate-oval__text");

        if (!path) return;

        const length = path.getTotalLength();

        gsap.set(path, {
          strokeDasharray: length,
          strokeDashoffset: length
        });

        const tl = gsap.timeline({ paused: true });

        tl.to(path, {
          strokeDashoffset: 0,
          duration: 1.6,
          ease: "power2.out"
        });

        wrap.addEventListener("mouseenter", () => {
          tl.play();
          gsap.to(text, { scale: 1.05, duration: 0.2 });
        });

