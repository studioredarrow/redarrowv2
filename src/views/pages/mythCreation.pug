  script.
    document.addEventListener("DOMContentLoaded", () => {
      document.querySelectorAll(".footer-oval").forEach((wrap) => {
        const path = wrap.querySelector(".footer-oval__path");
        const text = wrap.querySelector(".footer-oval__text");

        if (!path || !text) return;

        const length = path.getTotalLength();

        gsap.set(path, {
          strokeDasharray: length,
          strokeDashoffset: length
        });

        const tl = gsap.timeline({ paused: true });

        tl.to(path, {
          strokeDashoffset: 0,
          duration: 1.6,
          ease: "power2.out"
        });

        wrap.addEventListener("mouseenter", () => {
          tl.play();
          gsap.to(text, {
            scale: 1.05,
            duration: 0.2,
            ease: "power2.out"
          });
        });

        wrap.addEventListener("mouseleave", () => {
          tl.reverse();
          gsap.to(text, {
            scale: 1,
            duration: 0.2,
            ease: "power2.out"
          });
        });
      });
    });


