      document.querySelectorAll(".hover-oval").forEach(function (wrap) {
        const path = wrap.querySelector(".hover-oval__path");
        const text = wrap.querySelector(".hover-oval__text");

        if (!path) return;

        const length = path.getTotalLength();

        gsap.set(path, {
          strokeDasharray: length,
          strokeDashoffset: length
        });

        const tl = gsap.timeline({ paused: true });

        tl.to(path, {
          strokeDashoffset: 0,
          duration: 2.0,
          ease: "power2.out"
        });

        wrap.addEventListener("mouseenter", function () {
          tl.play();
          gsap.to(text, { scale: 1.05, duration: 0.2 });
        });

        wrap.addEventListener("mouseleave", function () {
          tl.reverse();
          gsap.to(text, { scale: 1, duration: 0.2 });
        });
      });
      const cookiePopup = document.querySelector(".cookie-popup");

            function showCookie() {
              gsap.to(cookiePopup, {
                yPercent: 0,
                opacity: 1,
                duration: 0.8,
                ease: "power3.out"
              });
            }

            function hideCookie() {
              gsap.to(cookiePopup, {
                yPercent: 100,
                opacity: 0,
                duration: 0.6,
                ease: "power3.in"
              });
            }
          window.addEventListener("load", () => {
            if (!localStorage.getItem("cookieAccepted")) {
              showCookie();
            }
          });

        document.querySelectorAll(".actions button").forEach(btn => {
          btn.addEventListener("click", () => {
            hideCookie();
          });
        });
        const journeyBtn = document.querySelector(".journey");

          if (journeyBtn) {
            journeyBtn.addEventListener("click", () => {
              hideCookie();
            });
          }
          const yummyBtn = document.querySelector(".yummy-wrap");

          if (yummyBtn) {
            yummyBtn.addEventListener("click", () => {
              hideCookie();
              localStorage.setItem("cookieAccepted", "true");
            });
          }
          const declineBtn = document.querySelector(".decline");

            if (declineBtn) {
              declineBtn.addEventListener("click", () => {
                hideCookie();
              });
            }
        document.querySelectorAll(".action-btn").forEach(btn => {
          btn.addEventListener("click", () => {
            const question = btn.dataset.question;

            if (!question) return;

            // Encode to safely pass in URL
            const encoded = encodeURIComponent(question);

            window.location.href = `/myth-journey?q=${encoded}`;
          });
        });




